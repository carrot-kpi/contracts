{
    "name": "@carrot/contracts",
    "version": "0.1.0",
    "description": "A smart contract solution to create scalar and boolean KPI tokens, by DXdao.",
    "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
    "license": "GPL-3.0-or-later",
    "private": true,
    "scripts": {
        "lint:eslint": "eslint \"test/**/*.ts\" \"tasks/**/*.ts\"",
        "lint:prettier": "prettier -l contracts/**/*.sol && prettier -l test/**/*.js",
        "lint:commit-message": "commitlint -e",
        "lint": "yarn lint:eslint && yarn lint:prettier",
        "test": "hardhat test",
        "test:coverage": "hardhat coverage",
        "test:gasreport": "cross-env GAS_REPORT_ENABLED=true hardhat test",
        "compile": "hardhat compile",
        "prepare": "husky install",
        "typechain": "typechain --outDir=typechain-types --target=ethers-v5 './artifacts/contracts/**/!(*.dbg).json'",
        "deploy:rinkeby": "hardhat deploy --network rinkeby",
        "deploy:rinkeby:verify": "hardhat deploy --network rinkeby --verify",
        "deploy:rinkeby:automation-enabled": "hardhat deploy --network rinkeby --verify --workers-jobs-registry-address 0x480401a34Fd72b6B0820d8C99C01E9Fc00B61f93",
        "upgrade-uniswap-v2-twap-template:rinkeby": "hardhat upgrade-uniswap-v2-twap-template --network rinkeby --verify --oracles-manager-address 0x79D68f2711D102124190473EDdF87B62fEFD2821",
        "upgrade-manual-reality-oracle-template:rinkeby": "hardhat upgrade-manual-reality-eth-oracle-template --network rinkeby --verify --oracles-manager-address 0x79D68f2711D102124190473EDdF87B62fEFD2821",
        "upgrade-erc20-kpi-token-template:rinkeby": "hardhat upgrade-erc20-kpi-token-template --network rinkeby --verify --kpi-tokens-manager-address 0xE97c1Ba0bD412eCB36aCfFa6D9E485Bf963e9AE0",
        "upgrade-aave-erc20-kpi-token-template:rinkeby": "hardhat upgrade-aave-erc20-kpi-token-template --network rinkeby --verify --kpi-tokens-manager-address 0xE97c1Ba0bD412eCB36aCfFa6D9E485Bf963e9AE0",
        "upgrade-manual-reality-eth-oracle-template:rinkeby": "hardhat upgrade-manual-reality-eth-oracle-template --network rinkeby --verify --oracles-manager-address 0x79D68f2711D102124190473EDdF87B62fEFD2821",
        "create-uniswap-v2-twap-kpi-token:rinkeby": "hardhat create-uniswap-v2-twap-kpi-token --network rinkeby --jolt-jobs-registry-address 0xA0b83E5fb2a53Aba490D84602CfD4094F7eE0a21 --factory-address 0x77D19A7A2D6332f791179beeA180e305B26b0198 --kpi-tokens-manager-address 0xE97c1Ba0bD412eCB36aCfFa6D9E485Bf963e9AE0 --oracles-manager-address 0x79D68f2711D102124190473EDdF87B62fEFD2821",
        "create-reality-eth-kpi-token:rinkeby": "hardhat create-reality-eth-kpi-token --network rinkeby --kpi-template-id 0 --oracle-template-id 0 --jolt-jobs-registry-address 0xA0b83E5fb2a53Aba490D84602CfD4094F7eE0a21 --workers-token-address 0x727fBc9Cb628Ef25cf8AB59aC4eac26c251f6Ed6 --factory-address 0x77D19A7A2D6332f791179beeA180e305B26b0198",
        "create-manual-reality-eth-erc20-kpi-token:rinkeby": "hardhat create-manual-reality-eth-erc20-kpi-token --network rinkeby --kpi-template-id 0 --oracle-template-id 1 --factory-address 0x77D19A7A2D6332f791179beeA180e305B26b0198 --kpi-tokens-manager-address 0xE97c1Ba0bD412eCB36aCfFa6D9E485Bf963e9AE0",
        "create-manual-reality-eth-aave-erc20-kpi-token:rinkeby": "hardhat create-manual-reality-eth-aave-erc20-kpi-token --network rinkeby --kpi-template-id 1 --oracle-template-id 1 --factory-address 0x77D19A7A2D6332f791179beeA180e305B26b0198 --kpi-tokens-manager-address 0xE97c1Ba0bD412eCB36aCfFa6D9E485Bf963e9AE0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@jolt-network/contracts": "^0.6.1",
        "@nomiclabs/hardhat-ethers": "^2.0.5",
        "@nomiclabs/hardhat-etherscan": "^3.0.1",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@openzeppelin/contracts": "^4.5.0",
        "@openzeppelin/contracts-upgradeable": "^4.5.1",
        "@realitio/realitio-contracts": "^2.2.9",
        "@typechain/ethers-v5": "^9.0.0",
        "@typechain/hardhat": "^4.0.0",
        "@types/chai": "^4.3.0",
        "@types/luxon": "^2.0.9",
        "@types/mocha": "^9.0.0",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "decimal.js-light": "^2.5.1",
        "dotenv": "^16.0.0",
        "eslint": "^8.9.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.4",
        "hardhat": "^2.8.4",
        "hardhat-dependency-compiler": "^1.1.2",
        "hardhat-gas-reporter": "^1.0.8",
        "husky": "^7.0.4",
        "luxon": "^2.3.0",
        "merkletreejs": "^0.2.27",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.20",
        "ts-generator": "^0.1.1",
        "ts-node": "^10.5.0",
        "typechain": "^7.0.0",
        "typescript": "^4.5.5"
    }
}
