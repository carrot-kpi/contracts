{
    "name": "@carrot/contracts",
    "version": "0.1.0",
    "description": "A smart contract solution to create scalar and boolean KPI tokens, by DXdao.",
    "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
    "license": "GPL-3.0-or-later",
    "private": true,
    "scripts": {
        "lint:eslint": "eslint \"test/**/*.ts\" \"tasks/**/*.ts\"",
        "lint:prettier": "prettier -l contracts/**/*.sol && prettier -l test/**/*.js",
        "lint:commit-message": "commitlint -e",
        "lint": "yarn lint:eslint && yarn lint:prettier",
        "test": "hardhat test",
        "test:coverage": "hardhat coverage",
        "test:gasreport": "cross-env GAS_REPORT_ENABLED=true hardhat test",
        "compile": "hardhat compile",
        "prepare": "husky install",
        "typechain": "typechain --outDir=typechain-types --target=ethers-v5 './artifacts/contracts/**/!(*.dbg).json'",
        "deploy:rinkeby": "hardhat deploy --network rinkeby",
        "deploy:rinkeby:verify": "hardhat deploy --network rinkeby --verify",
        "deploy:rinkeby:automation-enabled": "hardhat deploy --network rinkeby --verify --workers-jobs-registry-address 0x480401a34Fd72b6B0820d8C99C01E9Fc00B61f93",
        "upgrade-uniswap-v2-twap-template:rinkeby": "hardhat upgrade-uniswap-v2-twap-template --network rinkeby --verify --oracles-manager-address 0xf495eDD3fF5b5e7486B77bdFFC6BBAbD5B8A4358",
        "upgrade-manual-reality-oracle-template:rinkeby": "hardhat upgrade-manual-reality-eth-oracle-template --network rinkeby --verify --oracles-manager-address 0xf495eDD3fF5b5e7486B77bdFFC6BBAbD5B8A4358",
        "upgrade-erc20-kpi-token-template:rinkeby": "hardhat upgrade-erc20-kpi-token-template --network rinkeby --verify --kpi-tokens-manager-address 0xBdc296CF95e665f3685868B41Ff66B6791463e0C",
        "upgrade-manual-reality-eth-oracle-template:rinkeby": "hardhat upgrade-manual-reality-eth-oracle-template --network rinkeby --verify --oracles-manager-address 0xf495eDD3fF5b5e7486B77bdFFC6BBAbD5B8A4358",
        "create-uniswap-v2-twap-kpi-token:rinkeby": "hardhat create-uniswap-v2-twap-kpi-token --network rinkeby --kpi-template-address 0x5341b9Bf81C6A336131f4445734801380F7c7275 --oracle-template-id 0 --workers-master-address 0xEb26cF988Bcd36c62654D53Cc81D660ABA45CFF7 --factory-address 0x50aa0256d2aD23488e663536f3B7B679F7A2F4A1",
        "create-reality-eth-kpi-token:rinkeby": "hardhat create-reality-eth-kpi-token --network rinkeby --kpi-template-address 0x5341b9Bf81C6A336131f4445734801380F7c7275 --oracle-template-id 0 --workers-master-address 0xEb26cF988Bcd36c62654D53Cc81D660ABA45CFF7 --workers-token-address 0x727fBc9Cb628Ef25cf8AB59aC4eac26c251f6Ed6 --factory-address 0x50aa0256d2aD23488e663536f3B7B679F7A2F4A1",
        "create-manual-reality-eth-kpi-token:rinkeby": "hardhat create-manual-reality-eth-kpi-token --network rinkeby --kpi-template-id 0 --oracle-template-id 1 --factory-address 0x50aa0256d2aD23488e663536f3B7B679F7A2F4A1 --kpi-tokens-manager-address 0xBdc296CF95e665f3685868B41Ff66B6791463e0C"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.3",
        "@commitlint/config-conventional": "^16.0.0",
        "@nomiclabs/hardhat-ethers": "^2.0.4",
        "@nomiclabs/hardhat-etherscan": "^3.0.0",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@openzeppelin/contracts": "^4.4.2",
        "@openzeppelin/contracts-upgradeable": "^4.4.2",
        "@realitio/realitio-contracts": "^2.2.9",
        "@typechain/ethers-v5": "^9.0.0",
        "@typechain/hardhat": "^4.0.0",
        "@types/chai": "^4.3.0",
        "@types/luxon": "^2.0.9",
        "@types/mocha": "^9.0.0",
        "@xcute/contracts": "^0.4.0",
        "babel-eslint": "^10.1.0",
        "chai": "^4.2.0",
        "cross-env": "^7.0.3",
        "decimal.js-light": "^2.5.1",
        "dotenv": "^14.2.0",
        "eslint": "^8.7.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.3",
        "hardhat": "^2.8.2",
        "hardhat-dependency-compiler": "^1.1.2",
        "hardhat-gas-reporter": "^1.0.7",
        "husky": "^7.0.4",
        "luxon": "^2.3.0",
        "merkletreejs": "^0.2.27",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.18",
        "ts-generator": "^0.1.1",
        "ts-node": "^10.4.0",
        "typechain": "^7.0.0",
        "typescript": "^4.5.4"
    }
}
